public class AddProgram {
    public integer a=10;
    public integer b=20;
    public integer c=30;
    public integer add(){
        integer total1 = a+b+c;
        return total1;
    }


    public integer sub(){
        integer total2 = a-b-c;
        return total2;
    }


    public integer mul(){
        integer total3 = a*b*c;
        return total3;
    }
}